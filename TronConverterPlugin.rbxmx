--insert a decal into the game using the toolbox in Roblox Studio Edit.
--Click on the decal with your mouse
--and copy the TextureID to the parenthesees below
--If you need more frames, just keep copying and pasting the line below!
 ()]]

function conversion:CreateNew(basemodel,tv,tvtop,screen1,screen2,side1,side2,side3,side4,theme)
	local u = script.Template:Clone()
	if tv == nil then tv = " " end
	if tvtop == nil then tvtop = " " end
	if screen1 == nil then screen1 = " " end
	if screen2 == nil then screen2 = " " end
	if side1 == nil then side1 = " " end
	if side2 == nil then side2 = " " end
	if side3 == nil then side3 = " " end
	if side4 == nil then side4 = " " end
	if theme == nil then theme = " " end
	local function getSubbed(str)
		local k = string.gsub(str,checkstring," ") 
		local k1 = string.gsub(k,checkstring2," ")
		local ky = string.gsub(k1,checkstring7," ")
		local kx = string.gsub(ky,checkstring5,"wait")
		local kz = string.gsub(kx,checkstring6," ")
		local k2 = string.gsub(kz,checkstring3," ") 
		local k3 = string.gsub(k2,checkstring4," ")
		if k3 == nil then
			print(k3)
			k3 = " "
		end
		local h
		for i = 1,#k3 do
			if string.sub(k3,i,i+2) == " ()" then
				if string.sub(k3,i-1,i-1) ~= "n" then
					h = true
				end
			end
		end
		if h then
			warn("Tron: "..basemodel.Name.." is using improper format, check for any errors!")
		end
		return k3
	end
	tv = getSubbed(tv)
	tvtop = getSubbed(tvtop)
	screen1 = getSubbed(screen1)
	screen2 = getSubbed(screen2)
	side1 = getSubbed(side1)
	side2 = getSubbed(side2)
	side3 = getSubbed(side3)
	side4 = getSubbed(side4)
	if tv == nil then tv = " " end
	if tvtop == nil then tvtop = " " end
	if screen1 == nil then screen1 = " " end
	if screen2 == nil then screen2 = " " end
	if side1 == nil then side1 = " " end
	if side2 == nil then side2 = " " end
	if side3 == nil then side3 = " " end
	if side4 == nil then side4 = " " end
	if theme == nil then theme = " " end
	u.Name = basemodel.Name
	u.Source = [[
local Announce = false -- This will decide whether or not you will be announced. (True = announce, False = don't announce)

local AnnounceMessage = {"Approaching the ring...", "He is...", "REPLACE NAME HERE!"} -- These are you messages when being announced. Add as many messages as you'd like. (You can leave this blank)
		
local IntroTheme = ]]..theme..[[

local OutroTheme = ]]..theme..[[ -- If you don't have an intro theme, put your theme for both OutroTheme and IntroTheme OR put your IntroTheme as 0

local TronSides = {
TV = function() -- Add your TV IDs. You can copy and paste it from any old trons.
]]..tv..[[	
end,
		
TVTop = function() -- Add your TVTop IDs. You can copy and paste it from any old trons.
]]..tvtop..[[
end,
		
Screen1 = function() -- Add your Screen1 IDs. You can copy and paste it from any old trons.
]]..screen1..[[
end,
		
Screen2 = function() -- Add your Screen2 IDs. You can copy and paste it from any old trons.
]]..screen2..[[
end,
		
Side1 = function() -- Add your Side1 IDs. You can copy and paste it from any old trons.
]]..side1..[[
end,
		
Side2 = function() -- Add your Side2 IDs. You can copy and paste it from any old trons.
]]..side2..[[	
end,
		
Side3 = function() -- Add your Side3 IDs. You can copy and paste it from any old trons.
]]..side3..[[
end,
		
Side4 = function() -- Add your Side4 IDs. You can copy and paste it from any old trons.
]]..side4..[[
end}
-- [WARNING]
-- Editing anything after this point in the script can result in the script not working properly.
-- Feel free to tweak the script around, but do so at your own tron's risk!
function song(id,power)
	if power == true then
		local sound = Instance.new("Sound", workspace)
		sound.SoundId = "rbxassetid://" .. id
		sound.Name = "Theme"
		sound.Volume = 1
		sound:Play()
	else
		for i = 1,10 do
			for i,v in pairs (workspace:GetChildren()) do
				if v.Name == "Theme" and v.ClassName == "Sound" then
					v.Volume = v.Volume - 0.1
					wait(0.1)
				end
			end
		end
		for i,v in pairs (workspace:GetChildren()) do
			if v.Name == "Theme" and v.ClassName == "Sound" then
				v:Stop()
				v:Destroy()
			end
		end
	end
end
function announce()
	for i = 1,#AnnounceMessage do
		for _,v in pairs (game.Players:GetPlayers()) do
			local gui = Instance.new("ScreenGui",v.PlayerGui)
			local textObject = Instance.new("TextLabel", gui)
			textObject.Text = AnnounceMessage[i]
			textObject.Size = UDim2.new(1,0,1,0)
			textObject.BackgroundColor3 = Color3.new(0,0,0)
			textObject.BackgroundTransparency = 0.5
			textObject.TextColor3 = Color3.new(1,1,1)
			textObject.FontSize = Enum.FontSize.Size24
			textObject.TextWrapped = true
			game:GetService("Debris"):AddItem(gui,#AnnounceMessage[i]/8)
		end
		wait(#AnnounceMessage[i]/8)
	end
end
local screens,trigger = {"TV","TVTop","Screen1","Screen2","Side1","Side2","Side3","Side4"},script.Trigger
local function playScreens()
	for _,c in next, screens do
		if workspace:FindFirstChild(c) then
			trigger:Fire(c)
	    end
	end
end
local power = false
script.Trigger.Event:Connect(function(action, value)
	if action == "intro" then
		power = value
		if power == true then
			playScreens()
		end
		if IntroTheme == 0 then
			song(OutroTheme,power)
		else
			song(IntroTheme,power)
		end
		if Announce == true and value == true then
			announce()
		end
	elseif action == "main" then
		power = value
		playScreens()
		if OutroTheme == 0 then
			song(IntroTheme,power)
		else
			song(OutroTheme,power)
		end
	elseif TronSides[action] then
		repeat 
			TronSides[action]()
			wait()
		until power == false
	end
end)
