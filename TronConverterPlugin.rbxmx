local themedefault2 = "rbxassetid://"

function conversion:Transform(tron)
	local tv,tvtop,screen1,screen2,side1,side2,side3,side4,theme
	if typeof(tron) == "table" then return end
	if tron:FindFirstChild("Main") then
		for _,c in pairs(tron:GetDescendants()) do if string.find(c.Name,"TV") and not string.find(c.Name,"TVTop") then c.Name = "TV" end end
		for _,c in pairs(tron:GetDescendants()) do if string.find(c.Name,"TVTop") then c.Name = "TVTop" end end
		if tron.Main:FindFirstChild("TV") then
			if string.sub(1,#tron.Main.TV.Source) == string.sub(1,#default) then
				for _,c in pairs(tron:GetDescendants()) do
					if c.Name == "TV" and tv == nil then
						tv = c
					elseif c.Name == "TVTop" and tvtop == nil then
						tvtop = c
					elseif c.Name == "Screen1" and screen1 == nil then
						screen1 = c
					elseif c.Name == "Screen2" and screen2 == nil then
						screen2 = c
					elseif c.Name == "Side1" and side3 == nil then
						side1 = c
					elseif c.Name == "Side2" and side3 == nil then
						side2 = c
					elseif c.Name == "Side3" and side3 == nil then
						side3 = c
					elseif c.Name == "Side4" and side4 == nil then
						side4 = c
					end
				end
				local tvstring,tvtopstring,screen1string,screen2string,side1string,side2string,side3string,side4string = "","","","","","","",""
				if tv then
					for i = #default+1,#tv.Source do
						if string.sub(tv.Source,i,i+2) == "end" then 
							break 
						else
							tvstring = tvstring..string.sub(tv.Source,i,i)
						end
					end
				end
				if tvtop then
					for i = #default+1,#tvtop.Source do
						if string.sub(tvtop.Source,i,i+2) == "end" then 
							break 
						else
							tvtopstring = tvtopstring..string.sub(tvtop.Source,i,i)
						end
					end
				end
				if screen1 then
					for i = #default+1,#screen1.Source do
						if string.sub(screen1.Source,i,i+2) == "end" then 
							break 
						else
							screen1string = screen1string..string.sub(screen1.Source,i,i)
						end
					end
				end
				if screen2 then
					for i = #default+1,#screen2.Source do
						if string.sub(screen2.Source,i,i+2) == "end" then 
							break 
						else
							screen2string = screen2string..string.sub(screen2.Source,i,i)
						end
					end
				end
				if side1 then
					for i = #default+1,#side1.Source do
						if string.sub(side1.Source,i,i+2) == "end" then 
							break 
						else
							side1string = side1string..string.sub(side1.Source,i,i)
						end
					end
				end
				if side2 then
					for i = #default+1,#side2.Source do
						if string.sub(side2.Source,i,i+2) == "end" then 
							break 
						else
							side2string = side2string..string.sub(side2.Source,i,i)
						end
					end
				end
				if side3 then
					for i = #default+1,#side3.Source do
						if string.sub(side3.Source,i,i+2) == "end" then 
							break 
						else
							side3string = side3string..string.sub(side3.Source,i,i)
						end
					end
				end
				if side4 then
					for i = #default+1,#side4.Source do
						if string.sub(side4.Source,i,i+2) == "end" then 
							break 
						else
							side4string = side4string..string.sub(side4.Source,i,i)
						end
					end
				end
				if tron.Main:FindFirstChildWhichIsA("Sound") then
					local sound = tron.Main:FindFirstChildWhichIsA("Sound")
					if string.find(sound.SoundId,themedefault) then
						theme = string.sub(sound.SoundId,#themedefault+1,#sound.SoundId)
					elseif string.find(sound.SoundId,themedefault2) then
						theme = string.sub(sound.SoundId,#themedefault2+1,#sound.SoundId)
					end
				end
				tv = tvstring tvtop = tvtopstring screen1 = screen1string screen2 = screen2string side1 = side1string side2 = side2string side3 = side3string side4 = side4string 
				local j = conversion:CreateNew(tron,tv,tvtop,screen1,screen2,side1,side2,side3,side4,theme)
				repeat wait() until j ~= nil
				return true
			else
				warn("TV is not in proper format")
				return "TV is not in proper format"
			end
		else
			warn("TV is not a valid member of "..tron.Name..".Main")
			return "TV is not a valid member of "..tron.Name..".Main"
		end
	else
		warn("Main can't be found in "..tron.Name)
		return "Main can't be found in "..tron.Name
	end
end

return conversion
