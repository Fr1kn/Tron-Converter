				<ProtectedString name="Source"><![CDATA[local gui = script.Parent.Main:Clone()
gui.Parent = game.CoreGui
local enabled = false
plugin = plugin

cm = require(script.Parent.ConversionModule)
local selection = game:GetService("Selection")
local creator = plugin:CreateToolbar("TMB's Tron Converter"):CreateButton("Open Menu","Opens the Converter Menu!","rbxassetid://4751418303")
warn("Tron Converter Loaded, trons with improper format will have errors will converted but they are common sense fixes! (Removing parantheses or fixing an excess 'n' in a script) Improper format includes, but is not limited to: Commented text above functions and single script non-moving trons,")
defaultcolor1 = Color3.fromRGB(118, 255, 80)
defaultcolor2 = Color3.fromRGB(87, 189, 53)
activecolor1 = Color3.fromRGB(255, 28, 31)
activecolor2 = Color3.fromRGB(173, 19, 21)
--
wdcolor1 = Color3.fromRGB(31, 31, 31)
wdcolor2 = Color3.fromRGB(22, 22, 22)
wdcolor3 = Color3.fromRGB(47, 0, 0)
wdcolor4 = Color3.fromRGB(22, 0, 0)

if not game.ServerStorage:FindFirstChild("ConvertedTronsHolder") then
	s = Instance.new("Folder")
	s.Name = "ConvertedTronsHolder"
	s.Parent = game.ServerStorage
end

creator.Click:Connect(function()
	if not game.CoreGui:FindFirstChild("Main") then
		local m = script.Parent.Main:Clone()
		m.Parent = game.CoreGui
		gui = m
	end
	enabled = not enabled
	creator:SetActive(enabled)
	gui.Enabled = enabled
end)

gui.Frame.Button.TextButton.MouseButton1Click:Connect(function()
	if #selection:Get() == 1 then
		if selection:Get()[1]:IsA("Model") then
			local z = cm:Transform(selection:Get()[1])
			repeat wait() until z ~= nil
			if z then
				gui.Frame.Button.ImageColor3 = defaultcolor1
				gui.Frame.Button.WarningDisplay.ImageColor3 = defaultcolor2
				gui.Frame.Button.TextButton.Text = "Converted!"
				wait(2)
				gui.Frame.Button.TextButton.Text = "Convert!"
			else
				gui.Frame.Button.TextButton.Text = "Conversion Failed"
				gui.Frame.WarningDisplay.TextLabel.Text = z
				gui.Frame.WarningDisplay.ImageColor3 = wdcolor3
				gui.Frame.WarningDisplay.WarningDisplay.ImageColor3 = wdcolor4
				wait(2)
				gui.Frame.Button.ImageColor3 = defaultcolor1
				gui.Frame.Button.WarningDisplay.ImageColor3 = defaultcolor2
				gui.Frame.Button.TextButton.Text = "Convert!"
				wait(1)
				gui.Frame.WarningDisplay.TextLabel.Text = "No Warnings to Display"
				gui.Frame.WarningDisplay.ImageColor3 = wdcolor1
				gui.Frame.WarningDisplay.WarningDisplay.ImageColor3 = wdcolor2
			end
		else
			gui.Frame.WarningDisplay.TextLabel.Text = "Select a Tron Model!"
			gui.Frame.WarningDisplay.ImageColor3 = wdcolor3
			gui.Frame.WarningDisplay.WarningDisplay.ImageColor3 = wdcolor4
			wait(3)
			gui.Frame.WarningDisplay.TextLabel.Text = "No Warnings to Display"
			gui.Frame.WarningDisplay.ImageColor3 = wdcolor1
			gui.Frame.WarningDisplay.WarningDisplay.ImageColor3 = wdcolor2
		end
	elseif #selection:Get() > 1 then
		gui.Frame.Button.TextButton.Text = "Converting..."
		gui.Frame.Button.ImageColor3 = activecolor1
		gui.Frame.Button.WarningDisplay.ImageColor3 = activecolor2
		for _,c in pairs(selection:Get()) do
			if c:IsA("Model") then
				local z = cm:Transform(c)
				repeat wait() until z ~= nil
			end
		end
		gui.Frame.Button.ImageColor3 = defaultcolor1
		gui.Frame.Button.WarningDisplay.ImageColor3 = defaultcolor2
		gui.Frame.Button.TextButton.Text = "Converted!"
		wait(2)
		gui.Frame.Button.TextButton.Text = "Convert!"
	elseif #selection:Get() < 1 then
		gui.Frame.WarningDisplay.TextLabel.Text = "Select a Tron to Convert!"
		gui.Frame.WarningDisplay.ImageColor3 = wdcolor3
		gui.Frame.WarningDisplay.WarningDisplay.ImageColor3 = wdcolor4
		wait(3)
		gui.Frame.WarningDisplay.TextLabel.Text = "No Warnings to Display"
		gui.Frame.WarningDisplay.ImageColor3 = wdcolor1
		gui.Frame.WarningDisplay.WarningDisplay.ImageColor3 = wdcolor2
	end
end)

]]></ProtectedString>
